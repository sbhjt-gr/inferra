{
  "cli": {
    "version": ">=16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ANDROID_HOME": "/home/subhajit/Android/Sdk"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "env": {
        "ANDROID_HOME": "/home/subhajit/Android/Sdk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
