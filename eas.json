{
  "cli": {
    "version": ">= 15.0.9",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:bundleRelease",
        "withoutCredentials": false
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
